<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/accountMainFragment">
    <fragment
        android:id="@+id/accountMainFragment"
        android:name="com.example.remak.view.account.signIn.AccountMainFragment"
        android:label="account_main_fragment"
        tools:layout="@layout/account_main_fragment">
        <action
            android:id="@+id/action_accountMainFragment_to_accountSignUp1Fragment2"
            app:destination="@id/accountSignUp1Fragment2"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_accountMainFragment_to_accountEmailSignInFragment"
            app:destination="@id/accountEmailSignInFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/accountSignUp1Fragment2"
        android:name="com.example.remak.view.account.signUp.AccountSignUp1Fragment"
        android:label="account_signup1_fragment"
        tools:layout="@layout/account_signup1_fragment">
        <action
            android:id="@+id/action_accountSignUp1Fragment2_to_accountSignUp2Fragment2"
            app:destination="@id/accountSignUp2Fragment2"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountSignUp1Fragment2_to_accountMainFragment"
            app:destination="@id/accountMainFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"
            app:popUpToInclusive="false"
            app:restoreState="true" />
        <action
            android:id="@+id/action_accountSignUp1Fragment2_to_accountEmailSignInFragment"
            app:destination="@id/accountEmailSignInFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountSignUp2Fragment2"
        android:name="com.example.remak.view.account.signUp.AccountSignUp2Fragment"
        android:label="account_signup2_fragment"
        tools:layout="@layout/account_signup2_fragment">
        <action
            android:id="@+id/action_accountSignUp2Fragment2_to_accountSignUp3Fragment"
            app:destination="@id/accountSignUp3Fragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountSignUp2Fragment2_to_accountSignUp1Fragment2"
            app:destination="@id/accountSignUp1Fragment2"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/accountSignUp3Fragment"
        android:name="com.example.remak.view.account.signUp.AccountSignUp3Fragment"
        android:label="account_signup3_fragment"
        tools:layout="@layout/account_signup3_fragment">
        <action
            android:id="@+id/action_accountSignUp3Fragment_to_accountSignUp2Fragment2"
            app:destination="@id/accountSignUp2Fragment2"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountSignUp3Fragment_to_accountSignUp4Fragment"
            app:destination="@id/accountSignUp4Fragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/accountSignUp4Fragment"
        android:name="com.example.remak.view.account.signUp.AccountSignUp4Fragment"
        android:label="AccountSignUp4Fragment"
        tools:layout="@layout/account_signup4_fragment">
        <action
            android:id="@+id/action_accountSignUp4Fragment_to_accountSignUp3Fragment"
            app:destination="@id/accountSignUp3Fragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/accountEmailSignInFragment"
        android:name="com.example.remak.view.account.signIn.AccountEmailSignInFragment"
        android:label="AccountEmailSignInFragment"
        tools:layout="@layout/account_email_signin1_fragment">
        <action
            android:id="@+id/action_accountEmailSignInFragment_to_accountMainFragment"
            app:destination="@id/accountMainFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountEmailSignInFragment_to_accountSignUp1Fragment2"
            app:destination="@id/accountSignUp1Fragment2"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountEmailSignInFragment_to_accountResetPassword1Fragment"
            app:destination="@id/accountResetPassword1Fragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/accountResetPassword1Fragment"
        android:name="com.example.remak.view.account.signIn.AccountResetPassword1Fragment"
        android:label="AccountResetPassword1Fragment"
        tools:layout="@layout/account_reset_password1_fragment">
        <action
            android:id="@+id/action_accountResetPassword1Fragment_to_accountEmailSignInFragment"
            app:destination="@id/accountEmailSignInFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountResetPassword1Fragment_to_accountResetPassword2Fragment"
            app:destination="@id/accountResetPassword2Fragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/accountResetPassword2Fragment"
        android:name="com.example.remak.view.account.signIn.AccountResetPassword2Fragment"
        android:label="AccountResetPassword2Fragment"
        tools:layout="@layout/account_reset_password2_fragment">
        <action
            android:id="@+id/action_accountResetPassword2Fragment_to_accountResetPassword1Fragment"
            app:destination="@id/accountResetPassword1Fragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountResetPassword2Fragment_to_accountResetPassword3Fragment"
            app:destination="@id/accountResetPassword3Fragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"

            />
    </fragment>
    <fragment
        android:id="@+id/accountResetPassword3Fragment"
        android:name="com.example.remak.view.account.signIn.AccountResetPassword3Fragment"
        android:label="AccountResetPassword3Fragment"
        tools:layout="@layout/account_reset_password3_fragment">
        <action
            android:id="@+id/action_accountResetPassword3Fragment_to_accountResetPassword2Fragment"
            app:destination="@id/accountResetPassword2Fragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountResetPassword3Fragment_to_accountResetPassword4Fragment"
            app:destination="@id/accountResetPassword4Fragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/accountResetPassword4Fragment"
        android:name="com.example.remak.view.account.signIn.AccountResetPassword4Fragment"
        android:label="AccountResetPassword4Fragment"
        tools:layout="@layout/account_reset_password4_fragment">
        <action
            android:id="@+id/action_accountResetPassword4Fragment_to_accountResetPassword3Fragment"
            app:destination="@id/accountResetPassword3Fragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_accountResetPassword4Fragment_to_accountMainFragment"
            app:destination="@id/accountMainFragment" />
    </fragment>
</navigation>